@INBOOK{2009,
  pages = {417--424},
  title = {{Functional Neuroimaging Studies of Aging}},
  publisher = {Elsevier},
  year = {2009},
  abstract = {{Aging is characterized by changes in memory and other domains of
	cognitive function. Using functional neuroimaging techniques, we
	can go beyond behavioral measures and investigate what is happening
	in the brain during the performance of cognitive tasks. Comparing
	the task performance of younger and older adults under different
	conditions, and the brain activity that accompanies this performance,
	provides a means to understand cognitive aging. Young and older adults
	usually engage the same brain regions, but not always to the same
	extent. Older adults often recruit additional brain areas as well,
	indicating that additional cognitive resources, and perhaps compensatory
	mechanisms, need to be brought on line for task performance in the
	elderly.}},
