@ARTICLE{Rousseeuw1987,
  author = {Rousseeuw, Peter J.},
  title = {{Silhouettes: A graphical aid to the interpretation and validation
	of cluster analysis}},
  journal = {Journal of Computational and Applied Mathematics},
  year = {1987},
  volume = {20},
  pages = {53--65},
  number = {1},
  month = nov,
  abstract = {{A new graphical display is proposed for partitioning techniques.
	Each cluster is represented by a so-called silhouette, which is based
	on the comparison of its tightness and separation. This silhouette
