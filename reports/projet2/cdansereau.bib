  citeulike-article-id = {8804529},
  citeulike-linkout-0 = {http://dx.doi.org/10.1007/s10439-011-0258-9},
  citeulike-linkout-1 = {http://www.springerlink.com/content/6248t685l37k4731},
  day = {1},
  doi = {10.1007/s10439-011-0258-9},
  issn = {0090-6964},
  keywords = {alzheimer, basc, clustering, fmri, minimum-spanning-tree, resting-state,
	stability},
  owner = {cdansereau},
