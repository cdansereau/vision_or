  journal = {NeuroImage},
  year = {2007},
  volume = {38},
  pages = {306--320},
  number = {2},
  month = nov,
  abstract = {{Heart rate fluctuations occur in the low-frequency range (< 0.1 Hz)
	probed in functional magnetic resonance imaging (fMRI) studies of
	resting-state functional connectivity and most fMRI block paradigms
	and may be related to low-frequency blood-oxygenation-level-dependent
