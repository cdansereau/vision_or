@ARTICLE{Zhang2012,
  author = {Zhang, Sheng and Li, Chiang-shan R.},
  title = {{Functional connectivity mapping of the human precuneus by resting
	state fMRI}},
  journal = {NeuroImage},
  year = {2012},
  volume = {59},
  pages = {3548--3562},
  number = {4},
  month = feb,
  abstract = {{Precuneus responds to a wide range of cognitive processes. Here,
	we examined how the patterns of resting state connectivity may define
	functional subregions in the precuneus. Using a K-means algorithm
	to cluster the whole-brain '' correlograms'' of the precuneus in
	225 adult individuals, we corroborated the dorsal-anterior, dorsal-posterior,
	and ventral subregions, each involved in spatially guided behaviors,
	mental imagery, and episodic memory as well as self-related processing,
	with the ventral precuneus being part of the default mode network,
	as described extensively in earlier work. Furthermore, we showed
