@ARTICLE{Chen2006,
  author = {Chen, H. and Yuan, H. and Yao, D. and Chen, L. and Chen, W.},
  title = {{An integrated neighborhood correlation and hierarchical clustering
	approach of functional MRI.}},
  journal = {IEEE transactions on bio-medical engineering},
  year = {2006},
  volume = {53},
  pages = {452--458},
  number = {3},
  month = mar,
  abstract = {{Clustering analysis is a promising data-driven method for the analysis
	of functional magnetic resonance imaging (fMRI) time series, however,
	the huge computation load makes it difficult for practical use. In
	this paper, neighborhood correlation (NC) and hierarchical clustering
	(HC) methods are integrated as a new approach where fMRI data are
	processed first by NC to get a preliminary image of brain activations,
	and then by HC to remove some noises. In HC, to better use spatial
	and temporal information in fMRI data, a new spatio-temporal measure
	is introduced. A simulation study and an application to visual fMRI
	data show that the brain activations can be effectively detected
	and that different response patterns can be discriminated. These
	results suggest that the proposed new integrated approach could be
	useful in detecting weak fMRI signals.}},
