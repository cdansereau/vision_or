@ARTICLE{Shmueli2007,
  author = {Shmueli, Karin and van Gelderen, Peter and de Zwart, Jacco A. and
	Horovitz, Silvina G. and Fukunaga, Masaki and Jansma, J. Martijn
	and Duyn, Jeff H.},
  title = {{Low-frequency fluctuations in the cardiac rate as a source of variance
