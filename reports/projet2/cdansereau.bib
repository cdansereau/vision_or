  doi = {10.1016/j.neuroimage.2005.10.019},
  keywords = {bayesian, causality, connectivity, effective, fmri},
  owner = {cdansereau},
  posted-at = {2006-03-28 21:54:35},
  priority = {5},
  timestamp = {2014.09.16},
