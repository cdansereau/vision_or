@ARTICLE{Honey2009,
  author = {Honey, C. J. and Sporns, O. and Cammoun, L. and Gigandet, X. and
	Thiran, J. P. and Meuli, R. and Hagmann, P.},
  title = {{Predicting human resting-state functional connectivity from structural
